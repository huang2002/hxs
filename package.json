{
  "name": "3h-exp",
  "version": "0.1.0",
  "description": "A simple expression interpreter.",
  "module": "./dist/3h-exp.min.js",
  "main": "./dist/3h-exp.umd.min.js",
  "types": "./index.d.ts",
  "author": "3h",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc",
    "build": "rollup -c",
    "postbuild": "terser dist/3h-exp.js --config-file terser.json --module -o dist/3h-exp.min.js && terser dist/3h-exp.umd.js --config-file terser.json -o dist/3h-exp.umd.min.js && node stat",
    "test": "node ./test/index.js",
    "docs": "dts2md \"**\" \"!index.d.ts\" -i ./types -o ./docs -l -I Home.md"
  },
  "repository": "huang2002/3h-exp",
  "keywords": [
    "exp",
    "expression",
    "interpreter",
    "3h"
  ],
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "devDependencies": {
    "3h-test": "^0.4.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "dts2md": "^0.4.0",
    "rollup": "^2.35.1",
    "terser": "^5.5.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "3h-ast": "^0.3.2"
  }
}
